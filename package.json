{
  "name": "js-journal",
  "productName": "JS Journal",
  "version": "2.0.0",
  "description": "Journal app made with electronjs.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build -p never"
  },
  "build": {
    "appId": "com.willem.js-journal",
    "directories": {
      "output": "release"
    },
    "mac": {
      "artifactName": "js-journal-mac.${ext}",
      "category": "public.app-category.utility"
    },
    "win": {
      "artifactName": "js-journal-win.${ext}"
    },
    "linux": {
      "artifactName": "js-journal-linux.${ext}"
    }
  },
  "repository": "https://github.com/wnuke/js-journal",
  "author": "willem",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "crypto-js": "^3.1.9-1",
    "eslint-plugin-react": "^7.19.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0"
  }
}
